<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="190.0" prefWidth="600.0" style="-fx-background-color: #3D3F3C;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.musicverse.client.gui.MediaPlayerScreen">
   <children>
      <Slider fx:id="durationSlider" layoutX="31.0" layoutY="149.0" onMouseClicked="#sliderMouseClicked" prefHeight="14.0" prefWidth="534.0" />
      <Label fx:id="songLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="184.0" layoutY="20.0" prefHeight="18.0" prefWidth="231.0" text="Song name" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Label fx:id="albumLabel" alignment="CENTER" layoutX="168.0" layoutY="56.0" prefHeight="18.0" prefWidth="262.0" text="Album name" textFill="WHITE" />
      <Label fx:id="artistLabel" alignment="CENTER" layoutX="193.0" layoutY="74.0" prefHeight="18.0" prefWidth="211.0" text="Artist name" textFill="WHITE" />
      <Button fx:id="playButton" layoutX="241.0" layoutY="105.0" mnemonicParsing="false" onAction="#onPlayBtnClick" prefHeight="26.0" prefWidth="113.0" text="Play/Pause" />
      <Button fx:id="nextButton" layoutX="371.0" layoutY="106.0" mnemonicParsing="false" onAction="#onNextBtnClick" prefHeight="25.0" prefWidth="65.0" text="Next" />
      <Button fx:id="previousButton" layoutX="151.0" layoutY="106.0" mnemonicParsing="false" onAction="#onPreviousBtnClick" prefHeight="25.0" prefWidth="65.0" text="Previous" />
      <Label fx:id="leftSeconds" layoutX="531.0" layoutY="168.0" text="Label" textFill="WHITE" />
   </children>
</AnchorPane>
